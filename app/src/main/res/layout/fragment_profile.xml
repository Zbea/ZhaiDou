<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#f1f1f1">

    <RelativeLayout
        android:id="@+id/rl_actionbar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#39495d">
        <LinearLayout
            android:id="@+id/ll_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:orientation="horizontal">
            <ImageView
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:src="@drawable/icon_back"/>
            <com.zhaidou.view.TypeFaceTextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="16sp"
                android:drawablePadding="2dp"
                android:textColor="@color/white"
                android:layout_alignParentLeft="true"
                android:text="返回"/>
        </LinearLayout>
        <com.zhaidou.view.TypeFaceTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textSize="18sp"
            android:textColor="@color/white"
            android:text="个人资料"/>
    </RelativeLayout>
    <ScrollView
        android:layout_below="@id/rl_actionbar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"
        android:background="@color/transparent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
    <RelativeLayout
        android:id="@+id/rl_header_layout"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="80dp">
        <com.zhaidou.view.TypeFaceTextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:textColor="#666666"
            android:gravity="center_vertical"
            android:layout_marginLeft="20dp"
            android:text="更换头像"/>
        <LinearLayout
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">
            <com.zhaidou.view.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/iv_header"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_gravity="center_vertical"
                android:layout_marginRight="10dp"
                app:border_width="2dp"
                app:border_color="#d2d4d7"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_arrow_right"
            android:layout_marginRight="10dp"/>
        </LinearLayout>
        </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_nickname"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@color/white">
                <com.zhaidou.view.TypeFaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#666666"
                    android:text="我的昵称"
                    android:layout_centerVertical="true"
                    android:paddingLeft="20dp"/>
                <ImageView
                    android:id="@+id/iv_nick_arrow"
                    android:layout_width="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_height="wrap_content"
                    android:background="@drawable/icon_arrow_right"
                    android:layout_marginRight="10dp"/>
                <com.zhaidou.view.TypeFaceTextView
                    android:id="@+id/tv_nick"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#a8a8a8"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_toLeftOf="@id/iv_nick_arrow"
                    android:layout_marginRight="20dp"
                    android:paddingRight="10dp"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="20dp"
                    android:layout_alignParentBottom="true"
                    android:background="#eeeeee"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_email"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@color/white">
                <com.zhaidou.view.TypeFaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="注册邮箱"
                    android:textColor="#666666"
                    android:layout_centerVertical="true"
                    android:paddingLeft="20dp"/>
                <com.zhaidou.view.TypeFaceTextView
                    android:id="@+id/tv_email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#a8a8a8"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:paddingRight="10dp"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="20dp"
                    android:layout_alignParentBottom="true"
                    android:background="#eeeeee"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:visibility="gone"
                android:background="@color/white">
                <com.zhaidou.view.TypeFaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的性别"
                    android:textColor="#666666"
                    android:layout_centerVertical="true"
                    android:paddingLeft="20dp"/>
                <com.zhaidou.view.TypeFaceTextView
                    android:id="@+id/tv_gender"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#a8a8a8"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:paddingRight="10dp"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="20dp"
                    android:layout_alignParentBottom="true"
                    android:background="#eeeeee"/>
            </RelativeLayout>
            <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
                android:visibility="gone"
            android:background="@color/white">
            <com.zhaidou.view.TypeFaceTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的生日"
                android:textColor="#666666"
                android:layout_centerVertical="true"
                android:paddingLeft="20dp"/>
            <com.zhaidou.view.TypeFaceTextView
                android:id="@+id/tv_birthday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#a8a8a8"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:paddingRight="10dp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="20dp"
                android:layout_alignParentBottom="true"
                android:background="#eeeeee"/>
        </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:visibility="gone"
                android:background="@color/white">
                <com.zhaidou.view.TypeFaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的城市"
                    android:textColor="#666666"
                    android:layout_centerVertical="true"
                    android:paddingLeft="20dp"/>
                <com.zhaidou.view.TypeFaceTextView
                    android:id="@+id/tv_city"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#a8a8a8"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:paddingRight="10dp"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="20dp"
                    android:layout_alignParentBottom="true"
                    android:background="#eeeeee"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_job"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@color/white">
                <com.zhaidou.view.TypeFaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的职业"
                    android:textColor="#666666"
                    android:layout_centerVertical="true"
                    android:paddingLeft="20dp"/>
                <ImageView
                    android:id="@+id/iv_job_arrow"
                    android:layout_width="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_height="wrap_content"
                    android:background="@drawable/icon_arrow_right"
                    android:layout_marginRight="10dp"/>
                <com.zhaidou.view.TypeFaceTextView
                    android:id="@+id/tv_job"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#a8a8a8"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_toLeftOf="@id/iv_job_arrow"
                    android:layout_marginRight="20dp"
                    android:paddingRight="10dp"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="20dp"
                    android:layout_alignParentBottom="true"
                    android:background="#eeeeee"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:visibility="gone"
                android:background="@color/white">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="所属公司"
                    android:textColor="#666666"
                    android:layout_centerVertical="true"
                    android:paddingLeft="20dp"/>
                <TextView
                    android:id="@+id/tv_company"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#a8a8a8"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:paddingRight="10dp"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="20dp"
                    android:layout_alignParentBottom="true"
                    android:background="#eeeeee"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_mobile"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@color/white">
                <com.zhaidou.view.TypeFaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="手机号码"
                    android:textColor="#666666"
                    android:layout_centerVertical="true"
                    android:paddingLeft="20dp"/>
                <ImageView
                    android:id="@+id/iv_mobile_arrow"
                    android:layout_width="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_height="wrap_content"
                    android:background="@drawable/icon_arrow_right"
                    android:layout_marginRight="10dp"/>
                <com.zhaidou.view.TypeFaceTextView
                    android:id="@+id/tv_mobile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#a8a8a8"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_toLeftOf="@id/iv_mobile_arrow"
                    android:layout_marginRight="20dp"
                    android:paddingRight="10dp"/>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:visibility="gone"
                android:background="@color/white">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="注册时间"
                    android:textColor="#666666"
                    android:layout_centerVertical="true"
                    android:paddingLeft="20dp"/>
                <TextView
                    android:id="@+id/tv_register_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#a8a8a8"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:paddingRight="10dp"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_into"
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@color/white">
                <com.zhaidou.view.TypeFaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="个人简介"
                    android:textColor="#666666"
                    android:layout_centerVertical="true"
                    android:paddingLeft="20dp"/>
                <ImageView
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:background="@drawable/icon_arrow_right"
                    android:layout_marginRight="10dp"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="20dp"
                    android:layout_alignParentBottom="true"
                    android:background="#eeeeee"/>
            </RelativeLayout>
            <com.zhaidou.view.TypeFaceTextView
                android:id="@+id/tv_intro"
                android:background="@color/white"
                android:layout_width="match_parent"
                android:paddingLeft="20dp"
                android:textColor="#666666"
                android:paddingTop="5dp"
                android:layout_height="100dp" />
            <RelativeLayout
                android:id="@+id/rl_manage_address"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:background="@color/white">
                <com.zhaidou.view.TypeFaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:textColor="#666666"
                    android:paddingLeft="20dp"
                    android:gravity="center_vertical"
                    android:text="管理收货地址"/>
                <ImageView
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="10dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_arrow_right"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="20dp"
                android:background="#eeeeee"/>
            <LinearLayout
                android:id="@+id/ll_addr_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:background="@color/white"
                android:orientation="horizontal">
                <com.zhaidou.view.TypeFaceTextView
                    android:id="@+id/tv_addr_username"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="20dp"
                    android:textColor="#666666"/>
                <com.zhaidou.view.TypeFaceTextView
                    android:id="@+id/tv_addr_mobile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"/>
            </LinearLayout>
            <com.zhaidou.view.TypeFaceTextView
                android:id="@+id/tv_addr"
                android:background="@color/white"
                android:textColor="#666666"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:paddingTop="10dp"
                android:paddingLeft="20dp"
                android:paddingBottom="10dp"
                android:visibility="gone"
                android:layout_marginBottom="20dp"
                android:background="@color/white"
                android:layout_height="wrap_content">
                <com.zhaidou.view.TypeFaceTextView
                    android:layout_alignParentLeft="true"
                    android:layout_width="wrap_content"
                    android:textColor="#666666"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:drawableLeft="@drawable/icon_addr_checked"
                    android:drawablePadding="5dp"/>
                <com.zhaidou.view.TypeFaceTextView
                    android:id="@+id/tv_delete"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="删除"
                    android:textColor="#666666"
                    android:paddingRight="10dp"
                    android:drawableLeft="@drawable/delete"
                    android:drawablePadding="5dp"
                    android:gravity="center_vertical"
                    android:layout_alignParentRight="true"/>
                <com.zhaidou.view.TypeFaceTextView
                    android:id="@+id/tv_edit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="编辑"
                    android:textColor="#666666"
                    android:paddingRight="10dp"
                    android:drawablePadding="5dp"
                    android:layout_marginRight="10dp"
                    android:drawableLeft="@drawable/icon_edit"
                    android:gravity="center_vertical"
                    android:layout_toLeftOf="@id/tv_delete"/>
            </RelativeLayout>
        </LinearLayout>
        </LinearLayout>
    </ScrollView>
    <FrameLayout
        android:id="@+id/rl_header_menu"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <FrameLayout
        android:id="@+id/fl_child_container"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</RelativeLayout>
